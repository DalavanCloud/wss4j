        xmlns:wsp="http://www.w3.org/ns/ws-policy">
    <sp:TransportBinding wsp:Optional="true" wsp:Ignorable="true">
        <wsp:Policy>
            <sp:TransportToken>
                <wsp:Policy>
                </wsp:Policy>
            </sp:TransportToken>
            <sp:AlgorithmSuite>
