<wsp:Policy xmlns:wsp="http://www.w3.org/ns/ws-policy">
    <wsp:ExactlyOne>
        <wsp:All/>
        <wsp:All>
            <sp:Trust13 xmlns:sp="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702" wsp:Ignorable="true">
                <wsp:Policy xmlns:wsp="http://www.w3.org/ns/ws-policy">
                    <wsp:ExactlyOne>
                        <wsp:All>
                            <sp:MustSupportClientChallenge/>
                            <sp:MustSupportServerChallenge/>
                            <sp:RequireClientEntropy/>
                            <sp:RequireServerEntropy/>
                            <sp:MustSupportIssuedTokens/>
                            <sp:RequireRequestSecurityTokenCollection/>
                            <sp:RequireAppliesTo/>
                            <sp13:ScopePolicy15
                                    xmlns:sp13="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200802"></sp13:ScopePolicy15>
                            <sp13:MustSupportInteractiveChallenge
                                    xmlns:sp13="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200802"></sp13:MustSupportInteractiveChallenge>
                        </wsp:All>
                    </wsp:ExactlyOne>
                </wsp:Policy>
            </sp:Trust13>
        </wsp:All>
    </wsp:ExactlyOne>
</wsp:Policy>